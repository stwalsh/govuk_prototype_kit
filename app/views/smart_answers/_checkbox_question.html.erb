<ul class="options">
  <% question.options.each.with_index do |option, i| %>
    <li>
      <label for="response_<%= i %>">
        <%= check_box_tag "response[]", option.value, prefill_value_includes?(question, option.value), id: "response_#{i}" %>
        <%= option.label %>
      </label>
    </li>
 <% end %>
</ul>
